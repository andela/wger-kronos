{
    "name": "Wger-kronos",
    "description": "This app wger is a free and open source web application that manages your exercises and personal workouts, weight and diet plans.",
    "keywords": [
      "productivity",
      "HTML5",
      "scalpel"
    ],
    "website": "https://wger-kronos.herokuapp.com/",
    "repository": "https://github.com/andela/wger-kronos",
    "success_url": "/en/dashboard",
    "scripts": { },
    "env": {
        "HOST": {
          "required": true
        },
        "NAME": {
          "required": true
        },
        "PASSWORD": {
          "required": true
        },
        "PORT": {
          "required": true
        },
        "USER": {
          "required": true
        },
        "DISABLE_COLLECTSTATIC": {
          "required": true
        },
        "SECRET_KEY":{
          "required": true
        },
        "SOCIAL_AUTH_FACEBOOK_KEY":{
          "required": true
        },
        "SOCIAL_AUTH_FACEBOOK_SECRET":{
          "required": true
        },
        "SOCIAL_AUTH_GOOGLE_OAUTH2_KEY":{
          "required": true
        },
        "SOCIAL_AUTH_GOOGLE_OAUTH2_SECRET":{
          "required": true
        },
        "SOCIAL_AUTH_TWITTER_KEY":{
          "required": true
        },
        "SOCIAL_AUTH_TWITTER_SECRET":{
          "required": true
        }
      },
      "formation": {
        "web": {
          "quantity": 1
        }
      },
      "addons": [
          {
            "plan": "heroku-postgresql",
            "options": {
              "version": "9.5"
        }
      }
    ],
    "buildpacks": [
    {
        "url": "heroku/nodejs"
    },
    {
        "url": "heroku/python"
    }
    ],
    "stack": "heroku-18"
}
